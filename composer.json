{
    "name": "cyclonecode/wp-giosg",
    "type": "wordpress-plugin",
    "keywords": [
        "giosg",
        "chat",
        "live chat",
        "basket",
        "customer support",
        "woocommerce"
    ],
    "description": "This plugin integrates the live chat from giosg.com.",
    "require": {
        "php": ">=7.4"
    },
    "license": "GPL-2.0-or-later",
    "readme": "readme.txt",
    "homepage": "https://wordpress.org/plugins/wp-giosg/",
    "authors": [
        {
            "name": "Cyclonecode",
            "email": "cyclonecode@gmail.com",
            "role": "Maintainer"
        }
    ],
    "support": {
        "email": "cyclonecode.help@gmail.com",
        "issues": "https://wordpress.org/support/plugin/wp-giosg/",
        "source": "https://plugins.trac.wordpress.org/browser/wp-giosg/",
        "docs": "https://wordpress.org/plugins/wp-giosg/#description",
        "rss": "https://plugins.trac.wordpress.org/log/wp-giosg/?limit=100&mode=stop_on_copy&format=rss",
        "chat": "https://join.slack.com/t/cyclonecode/shared_invite/zt-6bdtbdab-n9QaMLM~exHP19zFDPN~AQ"
    },
    "autoload": {
        "psr-4": {
            "WPGiosg\\": [
                "src/"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^4.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5",
        "composer/installers": "^2.3"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    }
}
